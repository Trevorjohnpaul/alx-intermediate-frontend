{
  "compilerOptions": {
    "target": "es5",
    "module": "Node16",
    "outDir": "./dist",
    "rootDir": "./js",
    "strict": true,
    "noImplicitAny": true,
    "allowJs": true,
    "moduleResolution": "node16",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "ignoreDeprecations": "6.0"
  },
  "include": ["js/**/*.ts"],
  "exclude": ["node_modules", "dist", "webpack.config.js"]
}
